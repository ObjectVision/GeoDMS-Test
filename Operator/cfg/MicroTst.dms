			attribute<Base_grid_1km> UB1k := union_data(., point_yx(2867i, 2457i),point_yx(2867i, 2662i),point_yx(3276i, 2457i),point_yx(3276i, 2738i),point_yx(3566i, 2457i),point_yx(3564i, 2788i));
		}
		
		unit<ipoint> IrregularTiledUnit  := TiledUnit(StudyAreaTile/LB1k, StudyAreaTile/UB1k), DialogData = "BaseProjection" 
